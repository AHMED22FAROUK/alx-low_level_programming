#include <string.h>
#include "main.h"

/**
 * _strcmp - function to compare to strings
 *
 *
 *
 *
 */

int _strcmp(char *s1, char *s2)
{
	int i, cmp1, cmp2, k;

	k = strlen(s1);
	for (i = 0; i < k; i++)
	{
		cmp1 = s1[i];
		cmp2 = s2[i];
		if (cmp1 == cmp2)
		{
			continue;
		}
		else if (cmp1 > cmp2)
		{
			i = cmp1 - cmp2;
			break;
		}
		else
		{
			i = cmp1 - cmp2;
			break;
		}
	}
	return (i);
}
